{
  "root": true,
  "ignorePatterns": [
    "projects/**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "parserOptions": {
        "project": [
          "tsconfig.json"
        ],
        "createDefaultProgram": true,
        "ecmaVersion": "latest"
      },
      "env": {
        "es6": true
      },
      "extends": [
      ],
      "rules": {
        "no-unused-vars": false
      }
    },
    {
      "files": [
        "*.html"
      ],
      "extends": [
      ],
      "rules": {}
    }
  ]
}
